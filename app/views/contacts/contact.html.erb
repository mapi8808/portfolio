<h2 class="marker"><strong>お問い合わせフォーム</strong></h2>
  <%= form_with model: @contact do |f| %>
    <%= f.label :お名前（ユーザー名） %><br>
    <%= f.text_field :name %><br>
    <%= f.label :お問い合わせ内容 %><br>
    <%= f.text_field :message %><br>
    <%= f.submit '送信' %>
  <% end %>
    

<%= link_to 'HOME画面に戻る', root_path, class:"btn btn-secondary"  %>